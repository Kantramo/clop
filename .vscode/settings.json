// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true, // this excludes all folders 
                                 // named "node_modules" from 
                                 // the explore tree
        "lib": true // Don't search compiled code
    }
}